<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="account_analytic_account_view_form" model="ir.ui.view">
        <field name="name">account.analytic.account.form.inherit</field>
        <field name="model">account.analytic.account</field>
        <field name="inherit_id" ref="analytic.view_account_analytic_account_form" />
        <field name="arch" type="xml">
            <field name="plan_id" position="after">
                <field name="category_id" />
            </field>
        </field>
    </record>
    <record id="account_analytic_account_view_tree" model="ir.ui.view">
        <field name="name">account.analytic.account.tree.inherit</field>
        <field name="model">account.analytic.account</field>
        <field name="inherit_id" ref="analytic.view_account_analytic_account_list" />
        <field name="arch" type="xml">
            <field name="plan_id" position="after">
                <field name="category_id" optional="hide" />
            </field>
        </field>
    </record>
</odoo>
